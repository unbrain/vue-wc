{
  "name": "wc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "normalize.css": "^8.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-plugin": "^1.0.0",
    "postcss-pxtorem": "^4.0.1",
    "socket.io": "^2.2.0",
    "uws": "^100.0.1",
    "vue": "^2.6.6",
    "vue-socket.io": "^3.0.7",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-import": {},
      "postcss-mixins": {},
      "postcss-pxtorem": {
        "rootValue": 16,
        "propWhiteList": [
          "*",
          "!border",
          "!border-top",
          "!border-right",
          "!border-bottom",
          "!border-left",
          "!border-width"
        ],
        "selectorBlackList": [
          "html"
        ],
        "mediaQuery": false
      },
      "postcss-cssnext": {
        "features": {
          "rem": false
        }
      },
      "postcss-discard-comments": {},
      "postcss-discard-duplicates": {},
      "postcss-discard-empty": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
